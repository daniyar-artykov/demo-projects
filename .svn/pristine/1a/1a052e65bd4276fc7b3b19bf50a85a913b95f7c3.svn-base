package sleekbill.swingutils;

import java.util.Date;

/**
 *
 * @author  tonia
 */
public class SBDate extends Date {

    private boolean setLinie = false;

    public SBDate(Date date) {
        SBDate.this.setTime(date.getTime());
    }

    public SBDate(boolean setLinie) {
        this.setLinie = setLinie;
    }

    public boolean isLinie() {
        return setLinie;
    }
}
