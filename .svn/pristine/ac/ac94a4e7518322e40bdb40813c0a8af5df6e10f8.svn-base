	private void initDialog() {
		int vkCheck = Common.vKCheckAll(Common.connection);
		int nrZileExp = 31 - vkCheck;
		if (30 >= vkCheck) {
			jLabel1.setText("<html><center>You are currently using a TRIAL version with unlimited features that will <b>expire in X days</b>.<br>To keep using the <b>full version</b> of Sleek Bill select a <b>Premium License</b><br>orâ€¦<br>If you only need the basic options, give up all the awesome features and select a Free License.</html>");
			jLabel1.setText(jLabel1.getText().replace("X days", nrZileExp + (nrZileExp != 1 ? " days" : " day")));
		}
		if (Common.IS_PARTNER) {
			jButton1.setText("Contact or Purchase from a local partner");
			jLabel3.setText("Contact or Purchase from a local partner");
			jLabel6.setText("<html><a href=\"url\">Buy Online (safe and secure)</a></html>");
		}
		jLabel32.setText(jLabel32.getText().replace("x", "" + (Common.DOCUMENTS_COUNT + 1)));
		jButton1.setVisible(false);
		jButton2.setVisible(false);
		if (Common.selectedCompanyConfig.get(Config.INDIA).equals("free")) {
			jLabel36.setText("I remain Free and Limited");
		}
		pack();
	}

