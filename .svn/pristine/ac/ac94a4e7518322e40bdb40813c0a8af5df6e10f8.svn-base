    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
		if (Common.selectedCompanyConfig.get(Config.INDIA).equals("free")) {
			dispose();
			return;
		}

		int nResult = Messages.showQuestionMessage(this, "<html><b>You choose the Free License.</b><br>This means that you can only issue " + (Common.DOCUMENTS_COUNT + 1) + " documents per month and will not benefit from dedicated support.<br>" + "Are you sure you want to activate a Free license ?</html>", "Confirmation", new String[]{"I choose Free and accept the limits", "I want Premium with all Features"}, "I want Premium with all Features");
		if (nResult != -1) {
			new JDialogRegister(this, true, nResult != 0).setVisible(true);
		}
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
