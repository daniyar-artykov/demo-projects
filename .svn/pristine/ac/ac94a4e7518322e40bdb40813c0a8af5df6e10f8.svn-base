			new JDialogRegister(this, true, true).setVisible(true);
		} else {
			try {
				SBDesktop.browse(new java.net.URI("https://www.billingsoftware.in/partner-listing.html?utm_source=soft-buy&utm_medium=soft-sleek&utm_campaign=Sleek-Buy-Soft"));
			} catch (Exception ex) {
				ex.printStackTrace();
			}
		}
    }//GEN-LAST:event_jButton1ActionPerformed

